<Window x:Name="MainWindow" x:Class="R04_SlabsRebar.APIWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        
        ResizeMode="NoResize" 
        ShowInTaskbar="True" 
        WindowStartupLocation="CenterScreen"
        Height="600" Width="800"
        Topmost="False"
        Background="White"
        >
    <Window.Resources>
        <ResourceDictionary Source="./../Library/Resource/Styles.xaml"></ResourceDictionary>
    </Window.Resources>

  <StackPanel>
        <TextBlock Text="En cours......" Style="{StaticResource DataGridTextBlockCenter}" ></TextBlock>
  </StackPanel>
</Window>
